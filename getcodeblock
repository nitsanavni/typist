#!/usr/bin/env python3
import sys

def main():
    lines = sys.stdin.readlines()
    backtick_lines = [i for i, line in enumerate(lines) if line.startswith('```')]

    if not backtick_lines:
        # No ``` found, output everything
        for line in lines:
            print(line, end='')
    else:
        # Output lines between the first and last ``` lines
        start = backtick_lines[0] + 1
        end = backtick_lines[-1]
        for line in lines[start:end]:
            print(line, end='')

if __name__ == "__main__":
    main()
